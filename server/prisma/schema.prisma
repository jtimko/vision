generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  password String
  name     String?
  notes    Notes?
}

model Notes {
  id        Int     @id @default(autoincrement())
  note      String
  //status    Boolean @default(true)
  //createdDate DateTime @default(now())
  user      User   @relation(fields: [userId], references: [id])
  userId    Int     
}
